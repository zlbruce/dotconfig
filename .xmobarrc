Config { font = "xft:WenQuanYi Zen Hei Mono-12"
       , bgColor = "black"
       , fgColor = "grey"
       , position = TopSize L 90 25
       , lowerOnStart = True
       , commands = [ Run Weather "ZGSZ" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
                    , Run Network "eth0" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Network "eth1" ["-L","0","-H","32","--normal","green","--high","red"] 10
                    , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
		    , Run Volume "default" "Master" [] 10
		    , Run StdinReader
		    , Run PipeReader "/dev/shm/lrcfifo" "lrc"
                    , Run Com "uname" ["-s","-r"] "" 36000
    		    , Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%cpu% | %memory% | %eth0% | %StdinReader% }{ %lrc% | <fc=#ee9a00>%date%</fc> | %ZGSZ% | %default:Master%"
       }
